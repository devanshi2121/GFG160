Brute Foorce-
class Solution {
    public Node rotate(Node head, int k) {
        // add code here
        if(k==0 || head==null){
            return null;
        }
        for(int i=0;i<k;i++){
            Node curr=head;
            while(curr.next!=null){
                curr=curr.next;
            }
            curr.next=head;
            curr=curr.next;
            head=head.next;
            curr.next=null;
        }
        return head;
    }
}

Optimised-
class Solution {
    public Node rotate(Node head, int k) {
        // add code here
        if(k==0 || head==null){
            return head;
        }
        Node curr=head;
        int len=1;
        while(curr.next!=null){
            curr=curr.next;
            len++;
        }
        k%=len;
        if(k==0){
            return head;
        }
        curr.next=head;
        curr=head;
        
        for(int i=1;i<k;i++){
            curr=curr.next;
        }
        head=curr.next;
        curr.next=null;
        return head;
    }
}
